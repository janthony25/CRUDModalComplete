@model List<CRUDModal1.Models.Product>

@{
    ViewData["Title"] = "Product List";
}

<h3>@ViewData["Title"]</h3>

<hr />


<div class="container d-flex justify-content-between align-items-center mb-2 mt-5">
    <div class="left">

    </div>
    <div class="right">
        <button class="btn btn-primary" id="btnAdd">Add Product</button>
    </div>
</div>


<div class="container">
    <table class="table table-striped">
        <thead>
            <tr>
                <th>ID</th>
                <th>Product Name</th>
                <th>Price</th>
                <th>Qty</th>
                <th></th>
            </tr>
        </thead>
        <tbody id="tblBody">

        </tbody>
    </table>
</div>

@Html.Partial("_IndexModals")


<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/js/product.js"></script>